{
  "rootDir": "/",
  "port": 9001,
  "address": "127.0.0.1",
  "mongodb": {
    "host": "127.0.0.1",
    "port": 27017,
    "db": "skCube"
  },
  "pino": {
    "safe": true,
    "name": "gsrCollector",
    "timestamp": true,
    "prettyPrint": true,
    "serializers": {
      "req": "pino.stdSerializers.req",
      "res": "pino.stdSerializers.res"
    }
  },
  "gsr": {
    "file": {
      "maxSize": 266,
      "maxLength": 50,
      "storageFolder": "/gsrStorage"
    }
  },
  "limits": {
    "callsign": {
      "source": 6,
      "destination": 6
    },
    "meta": 20
  }
}
